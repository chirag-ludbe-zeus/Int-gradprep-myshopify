    "16383799536dcfe619": {
      "type": "faq",
      "blocks": {
        "70dea756-471e-43fc-87ff-b874e945e829": {
          "type": "faq",
          "settings": {
            "question": "Can I study online for graduate school entrance exams?",
            "answer": "GradPrep.com offers you all you need to study for your graduate school entrance exam online, anytime and at your own pace. We take the stress out of preparing you for your graduate school entrance exam."
          }
        },
        "01bcc8ba-8043-48ac-87a6-82a44a48bb12": {
          "type": "faq",
          "settings": {
            "question": "How much time do I need to study for graduate school entrance exams?",
            "answer": "GradPrep.com helps you figure out how to make the most of the time you have to prepare for your graduate school entrance exams. We help you prioritize what matters most for your specific needs."
          }
        },
        "13dad5c0-0b1a-41bd-92c2-0bb3a055f264": {
          "type": "faq",
          "settings": {
            "question": "Do you have graduate school entrance exam practice tests?",
            "answer": "GradPrep.com offers exclusive practice tests for the GMAT, MCAT, LSAT, and GRE. You can simulate the test day experience and prepare yourself best for what it will be like that day."
          }
        },
        "6c531edd-067d-4a2c-8a9c-7bb54012e771": {
          "type": "faq",
          "settings": {
            "question": "Do I need to take a graduate school entrance exam?",
            "answer": "Graduate school entrance exams continue to be a common language for graduate school applications and may also be required for financial aid requests."
          }
        },
        "6a50a658-886c-4960-8d13-15558f5394d4": {
          "type": "faq",
          "settings": {
            "question": "Which graduate school entrance exams does Grad Prep cover?",
            "answer": "We currently offer focused materials for preparing for the MCAT, LSAT, GMAT, and GRE."
          }
        },
        "298049ec-ca37-478e-a3dd-3d25fd840bb4": {
          "type": "faq",
          "settings": {
            "question": "What's the best way to prepare for a graduate school entrance exam?",
            "answer": "Practice tests that simulate the test day experience are the best way you can set yourself up for success. Then, focus your studies specifically on the areas you need to refresh."
          }
        }
      },
